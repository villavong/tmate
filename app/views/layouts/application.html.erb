<!DOCTYPE html>
<html>
<head>
  <title>TweedleMate.com</title>


  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <link href="//fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <script src="//use.fontawesome.com/f56787f761.js"></script>

</head>
<body style="">

<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '185078888571444',
      xfbml      : true,
      version    : 'v2.6'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>
<%= render 'shared/navbar' %>

<!-- <%= render 'shared/message' %> -->

<%= render 'shared/navbar2' %>




<% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: [:alert, alert_for(name)] %>
  <% end %>

<%= yield %>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-88959465-1', 'auto');
  ga('send', 'pageview');

</script>


</body>
<% if current_page?(root_path) %>
<footer class="clearfix" style="width:100%;padding:1rem 0;text-align:center;background-color:black;clear:both;bottom:0;left:0;  ">
  Contact : hello@tweedlemate.com <br>Kakao: soethan89
</footer>
<% end %>
</html>
